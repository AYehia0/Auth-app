<% if (typeof errs != "undefined") { %>
<%      errs.forEach(function (error) { %>
    <div class="alert alert-warning alert-dismissible show" role="alert">
        <%= error.errorMsg  %>
        <!-- <button type="button" class="" data-bs-dismiss="alert" aria-label="Close"></button> -->
    </div>
<%  }); %>
<% } %>